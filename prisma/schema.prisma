// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Transaction{
  id String @id @default(cuid())
  type String
  amount Int
  userEmail String
  category String
  remarks String?
  date DateTime @default(now())
}
model defCategories{
  id String @id @default(cuid())
  category String
  budget String

}
model userCategories{
  id String @id @default(cuid())
  category String
  budget String
  userEmail String
}